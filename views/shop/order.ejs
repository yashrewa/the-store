<%- include('../includes/head.ejs') %>

<body>
    <%- include('../includes/navigation.ejs')%>
    <main>
        <% if (orders.length > 0) {%>
        <ul>
            <% orders.forEach((order)=>{ %>
            <li>
                <div><%=order.id%></div>
                <ul>
                    <% order.products.forEach((product)=>{ %>
                        <li><%= product.title %></li>
                    <%})%>
                </ul>

            </li>
            <%})%>
        </ul>

        <%} else {%>
        <div>There is no item to display</div>
        <%}%>
        
    </main>
<%- include('../includes/end.ejs')%>